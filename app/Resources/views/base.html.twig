<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="{{ asset('libraries/bootstrap/css/bootstrap.css') }}" />
            <link rel="stylesheet" href="{{ asset('libraries/bootstrap-datetimepicker/bootstrap-datetimepicker.min.css') }}" />
            <link rel="stylesheet" href="{{ asset('libraries/bootstrap-slider/bootstrap-slider.min.css') }}" />
            <link rel="stylesheet" href="{{ asset('libraries/leaflet-routing-machine/leaflet-routing-machine.css') }}" />

            {#<link href="https://api.tiles.mapbox.com/mapbox.js/v2.1.9/mapbox.css" rel="stylesheet" />#}

            <link rel="stylesheet" href="{{ asset('css/leaflet.css') }}" />
            <link rel="stylesheet" href="{{ asset('css/main.css') }}" />
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>

    <div class="navbar navbar-default navbar-static-top" role="navigation">
        <div class="container-fluid">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#myNavbar" aria-expanded="false" aria-controls="navbar">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#">Routeviz</a>
            </div>
            <div class="navbar-collapse collapse" id="myNavbar">
                <ul class="nav navbar-nav">
                    <li class="nav-item">
                        <a class="nav-link" href="{{ path('index') }}">Real time positions</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="{{ path('historical') }}"> Historical data</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="{{ path('clustering') }}"> Clustering</a>
                    </li>
                    {#<li><button type="button" id="sidebarCollapse" class="btn btn-info navbar-btn">#}
                            {#<i class=""></i>#}
                            {#<span>Toggle Sidebar</span>#}
                        {#</button>#}
                    {#</li>#}
                </ul>
            </div><!--/.navbar-collapse -->
        </div>
    </div>

    <div id="container">
        <nav id="sidebar">
            <ul class="list-unstyled components">
                {% block sidebar %}
                {% endblock %}
            </ul>

        </nav>
        <div id="mapid"></div>

        <div class="overlay">
            <div id="loading-img"></div>
        </div>
    </div>


    {% block body %}

    {% endblock %}




    {% block javascripts %}

        {#<script src="{{ asset('build/manifest.js') }}"></script>
        <script src="{{ asset('build/vendor.js') }}"></script>

        <script src="{{ asset('build/app.js') }}"></script>
        <script src="{{ asset('build/map.js') }}"></script>#}

        <script src="{{ asset('js/jquery.min.js') }}"></script>
        <script src="{{ asset('js/moment.min.js') }}"></script>
        <script src="{{ asset('libraries/bootstrap/js/bootstrap.js') }}"></script>

        <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
        <script src="{{ path('fos_js_routing_js', { callback: 'fos.Router.setData' }) }}"></script>

        <script src="{{ asset('js/app.js') }}"></script>

        {#Leaflet map and heatmap for leaflet#}
        <script src="{{ asset('js/leaflet.js') }}"></script>
        <script src="{{ asset('libraries/leaflet-routing-machine/leaflet-routing-machine.min.js') }}"></script>

        <script src="{{ asset('js/leaflet-heat.js') }}"></script>


        {#<script src="https://api.tiles.mapbox.com/mapbox.js/v2.1.9/mapbox.js"></script>#}


        <script src="{{ asset('libraries/bootstrap-datetimepicker/bootstrap-datetimepicker.min.js') }}"></script>
        <script src="{{ asset('libraries/bootstrap-slider/bootstrap-slider.js') }}"></script>


        <script src="{{ asset('js/jDBSCAN.js') }}"></script>
        <script src="{{ asset('js/map.js') }}"></script>


    {% endblock %}
    </body>
</html>
